

class Solution {
public:
    int differenceOfSum(vector<int>& nums) {
        int n = nums.size(), sum = 0, sumDigits = 0;
        for(int i = 0; i < n; i++){
            sum += nums[i];
            int digits = nums[i];
            while(digits){
                sumDigits += digits % 10;
                digits = digits/10;
            }
        }
        return abs(sum - sumDigits);
    }
};
